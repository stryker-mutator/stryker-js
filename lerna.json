{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "packages": ["packages/*"],
  "version": "9.5.0",
  "npmClient": "pnpm",
  "command": {
    "version": {
      "allowBranch": ["master", "epic/*", "feat/*"],
      "conventionalCommits": true,
      "createRelease": "github",
      "forcePublish": true,
      "exact": true,
      "preid": "beta"
    }
  }
}
