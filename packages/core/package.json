    "log4js": "~6.7.0",
    "minimatch": "~5.1.0",
    "mkdirp": "~1.0.3",
    "mutation-testing-elements": "1.7.12",
    "mutation-testing-metrics": "1.7.10",
    "mutation-testing-report-schema": "1.7.10",
    "npm-run-path": "~5.1.0",
    "progress": "~2.0.0",
    "rimraf": "~3.0.0",
