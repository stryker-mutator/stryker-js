{
  "name": "@stryker-mutator/core",
  "version": "9.4.0",
  "description": "The extendable JavaScript mutation testing framework",
  "type": "module",
  "files": [
    "bin",
    "dist/src",
    "schema",
    "src"
  ],
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "tsc -w",
    "test": "c8 npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha \"dist/test/unit/**/*.js\"",
    "test:integration": "mocha --timeout 60000 \"dist/test/integration/**/*.js\"",
    "stryker": "node bin/stryker.js run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/stryker-js.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mutation testing",
    "mutation",
    "testing",
    "test",
    "js",
    "stryker"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Simon de Lang",
  "contributors": [
    "Simon de Lang <simondelang@gmail.com>",
    "Nico Jansen <jansennico@gmail.com>",
    "Sander Koenders <sanderkoenders@gmail.com>",
    "Philipp Weissenbacher <philipp.weissenbacher@gmail.com>",
    "Jasper Catthoor <jasper.catthoor@gmail.com>",
    "Nico Stapelbroek <nstapelbroek@gmail.com>",
    "Alex van Assem <avassem@gmail.com>",
    "Jelle Peters <JellePetersHAN@users.noreply.github.com>",
    "Jeremy Nagel <jeremy.nagel@learnosity.com>",
    "MarktHart <git@markhart.nl>",
    "Michael Williamson <mike@zwobble.org>",
    "Willem Meints <willem@mein-design.nl>",
    "Sharikov Vladislav <sharikov.vladislav@gmail.com>",
    "Jonathan Presson <jonathan.presson@gmail.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker-js/issues"
  },
  "homepage": "https://stryker-mutator.io/",
  "bin": {
    "stryker": "./bin/stryker.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.0",
    "@stryker-mutator/api": "workspace:*",
    "@stryker-mutator/instrumenter": "workspace:*",
    "@stryker-mutator/util": "workspace:*",
    "ajv": "~8.17.1",
    "chalk": "~5.6.0",
    "commander": "~14.0.0",
    "diff-match-patch": "1.0.5",
    "emoji-regex": "~10.6.0",
    "execa": "~9.6.0",
    "json-rpc-2.0": "^1.7.0",
    "lodash.groupby": "~4.6.0",
    "minimatch": "~10.1.0",
    "mutation-server-protocol": "~0.4.0",
    "mutation-testing-elements": "3.7.1",
    "mutation-testing-metrics": "3.7.1",
    "mutation-testing-report-schema": "3.7.1",
    "npm-run-path": "~6.0.0",
    "progress": "~2.0.3",
    "rxjs": "~7.8.1",
    "semver": "^7.6.3",
    "source-map": "~0.7.4",
    "tree-kill": "~1.2.2",
    "tslib": "2.8.1",
    "typed-inject": "~5.0.0",
    "typed-rest-client": "~2.2.0"
  },
  "devDependencies": {
    "@stryker-mutator/test-helpers": "workspace:*",
    "@types/diff-match-patch": "1.0.36",
    "@types/json-schema": "7.0.15",
    "@types/lodash.groupby": "4.6.9",
    "@types/node": "24.10.8",
    "@types/progress": "2.0.7",
    "@types/semver": "7.7.1",
    "glob": "13.0.0"
  }
}
