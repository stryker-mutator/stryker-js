    "mkdirp": "~0.5.1",
    "prettier": "~1.16.1",
    "progress": "~2.0.0",
    "rimraf": "~2.6.1",
    "rxjs": "~6.3.0",
    "source-map": "~0.6.1",
    "surrial": "~1.0.0",
    "tree-kill": "~1.2.0",
    "tslib": "~1.9.3",
    "typed-inject": "~1.0.0",
    "typed-rest-client": "~1.1.2"
  },
  "devDependencies": {
    "@stryker-mutator/api": "^0.24.1",
    "@stryker-mutator/test-helpers": "^1.0.1",
    "@types/get-port": "~4.0.0",
    "@types/inquirer": "~0.0.42",
