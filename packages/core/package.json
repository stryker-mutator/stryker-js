    "mkdirp": "~1.0.3",
    "mutation-testing-elements": "1.7.10",
    "mutation-testing-metrics": "1.7.10",
    "npm-run-path": "~5.1.0",
    "progress": "~2.0.0",
    "rimraf": "~3.0.0",
    "rxjs": "~7.5.1",
    "semver": "^7.3.5",
    "source-map": "~0.7.3",
    "tree-kill": "~1.2.2",
    "tslib": "~2.3.0",
