{
  "name": "stryker-jasmine",
  "version": "0.10.8",
  "description": "A plugin to use the Jasmine test framework in Stryker, the JavaScript mutation testing framework",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc -w",
    "clean": "rimraf \"src/**/+(*.d.ts|*.js|*.map)\" \"test/**/+(*.d.ts|*.js|*.map)\" .nyc_output reports",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "tslint -p tsconfig.json",
    "test": "nyc --reporter=html --report-dir=reports/coverage --check-coverage --lines 100 --functions 100 --branches 100  npm run mocha",
    "mocha": "mocha \"test/helpers/**/*.js\" \"test/unit/**/*.js\" && mocha --timeout 10000 \"test/helpers/**/*.js\" \"test/integration/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stryker-mutator/stryker.git"
  },
  "keywords": [
    "stryker",
    "jasmine",
    "stryker-plugin",
    "stryker-test-framework",
    "stryker-karma-runner",
    "stryker-jasmine-runner"
  ],
  "author": "Nico Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker/issues"
  },
  "homepage": "https://github.com/stryker-mutator/stryker/tree/master/packages/stryker-jasmine#readme",
  "peerDependencies": {
    "jasmine-core": ">=2",
    "stryker-api": ">=0.18.0 <0.23.0"
  },
  "devDependencies": {
    "stryker-api": "^0.22.0"
  },
  "contributors": []
}
