{
  "name": "stryker-jasmine-runner",
  "version": "0.4.0",
  "description": "A plugin to use the Jasmine (NodeJS) test runner in Stryker, the mutation testing framework for JavaScript and friends",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc -w",
    "clean": "rimraf \"src/**/+(*.d.ts|*.js|*.map)\" \"test/**/+(*.d.ts|*.js|*.map)\" .nyc_output reports",
    "test": "nyc --reporter=html --report-dir=reports/coverage --lines 100 --functions 100 --branches 85 npm run mocha",
    "mocha": "mocha \"test/helpers/**/*.js\" \"test/unit/**/*.js\" \"test/integration/**/*.js\""
  },
  "nyc": {
    "exclude": [
      "test",
      "testResources"
    ]
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/stryker-mutator/stryker.git"
  },
  "keywords": [
    "stryker",
    "jasmine",
    "stryker-plugin",
    "stryker-test-runner"
  ],
  "author": "Nico Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker/issues"
  },
  "homepage": "https://github.com/stryker-mutator/stryker/tree/master/packages/stryker-jasmine-runner#readme",
  "peerDependencies": {
    "jasmine": ">=2"
  },
  "devDependencies": {
    "@stryker-mutator/test-helpers": "^0.1.0",
    "stryker-jasmine": "^0.12.0"
  },
  "dependencies": {
    "stryker-api": "^0.24.0"
  },
  "initStrykerConfig": {
    "jasmineConfigFile": "spec/support/jasmine.json"
  },
  "contributors": []
}
