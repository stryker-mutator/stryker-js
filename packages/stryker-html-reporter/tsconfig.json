{
  "extends": "../../tsconfig.settings.json",
  "compilerOptions": {
    "rootDir": ".",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.core",
      "es2015.symbol",
      "es2015.symbol.wellknown"
    ],
    "noImplicitReturns": false,
    "jsx": "react",
    "jsxFactory": "typedHtml.createElement"
  },
  "exclude": [
    "node_modules",
    "src/**/*.d.ts",
    "test/**/*.d.ts",
    "testResources"
  ],
  "references": [
    {
      "path": "../stryker-api",
    },
    {
      "path": "../stryker-util",
    }
  ]
}