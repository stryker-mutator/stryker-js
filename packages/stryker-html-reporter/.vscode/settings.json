{
  "typescript.tsdk": "${workspaceRoot}/../../node_modules/typescript/lib",
  "files": {
    "exclude": {
      ".git": "",
      "**/*.js": {
        "when": "$(basename).ts"
      },
      "**/**.js": {
        "when": "$(basename).tsx"
      },
      "**/*.d.ts": true,
      "**/*.map": {
        "when": "$(basename)"
      }
    }
  }
}