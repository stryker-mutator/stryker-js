{
  "name": "stryker-babel-transpiler",
  "version": "0.7.10",
  "description": "A plugin for babel projects using Stryker",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc -w",
    "clean": "rimraf \"+(test|src)/**/*+(.d.ts|.js|.map)\" reports",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "tslint -p tsconfig.json",
    "test": "nyc --reporter=html --report-dir=reports/coverage --check-coverage --lines 85 --functions 90 --branches 80 npm run mocha",
    "mocha": "mocha \"test/helpers/**/*.js\" \"test/unit/**/*.js\" \"test/integration/**/*.js\""
  },
  "nyc": {
    "exclude": [
      "test",
      "testResources"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stryker-mutator/stryker"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "stryker",
    "stryker-plugin",
    "javascript",
    "stryker-transpiler",
    "babel"
  ],
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker/issues"
  },
  "author": "Sander Koenders <sanderkoenders@gmail.com",
  "contributors": [
    "Nico Jansen <jansennico@gmail.com>",
    "Simon de Lang <simondelang@gmail.com>",
    "Niek te Grootenhuis <ntegro@hotmail.com>",
    "Thomas Peters <thomaspeters4@outlook.com>"
  ],
  "homepage": "https://github.com/stryker-mutator/stryker/tree/master/packages/stryker-babel-transpiler#readme",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-core": "6.26.3",
    "minimatch": "~3.0.4"
  },
  "devDependencies": {
    "@types/babel-core": "~6.25.3",
    "@types/glob": "~7.1.0",
    "@types/minimatch": "~3.0.3",
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-es2015-spread": "~6.22.0",
    "babel-preset-es2015": "~6.24.1",
    "glob": "~7.1.2",
    "stryker-api": "^0.22.0"
  },
  "peerDependencies": {
    "babel-core": "^6.26.0 || ^7.0.0-bridge.0",
    "stryker-api": ">=0.18.0 <0.23.0"
  },
  "initStrykerConfig": {
    "babelrcFile": ".babelrc",
    "coverageAnalysis": "off"
  }
}
