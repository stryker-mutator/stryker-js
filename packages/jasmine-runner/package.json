{
  "name": "@stryker-mutator/jasmine-runner",
  "version": "5.4.0",
  "description": "A plugin to use the Jasmine (NodeJS) test runner in Stryker, the mutation testing framework for JavaScript and friends",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "nyc npm run test:all",
    "test:all": "mocha \"dist/test/**/*.js\"",
    "stryker": "node ../core/bin/stryker run"
  },
  "nyc": {
    "exclude": [
      "test",
      "testResources"
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stryker-mutator/stryker-js.git",
    "directory": "packages/jasmine-runner"
  },
  "keywords": [
    "stryker",
    "jasmine",
    "@stryker-mutator/test-runner-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Nico Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/stryker-js/issues"
  },
  "homepage": "https://stryker-mutator.io/docs/stryker-js/jasmine-runner",
  "peerDependencies": {
    "@stryker-mutator/core": "~5.4.0",
    "jasmine": ">=2"
  },
  "devDependencies": {
    "@stryker-mutator/test-helpers": "5.4.0",
    "@types/node": "^16.9.1"
  },
  "dependencies": {
    "@stryker-mutator/api": "5.4.0",
    "@stryker-mutator/util": "5.4.0"
  },
  "initStrykerConfig": {
    "jasmineConfigFile": "spec/support/jasmine.json"
  },
  "contributors": []
}
