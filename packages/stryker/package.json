{
  "name": "stryker",
  "version": "0.6.0-2",
  "description": "The extendable JavaScript mutation testing framework",
  "main": "src/Stryker.js",
  "typings": "src/Stryker.d.ts",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rimraf \"+(test|src)/**/*+(.d.ts|.js|.map)\" coverage testResources/module/node_modules/stryker",
    "build": "tsc -p .",
    "postbuild": "tslint -p tsconfig.json",
    "test": "nyc --check-coverage --lines 80 --functions 80 --branches 75 mocha \"test/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stryker-mutator/stryker"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "mutation testing",
    "mutation",
    "testing",
    "test",
    "js",
    "stryker"
  ],
  "author": "Simon de Lang",
