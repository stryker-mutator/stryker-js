{
  "name": "stryker-parent",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/glob": "8.0.0",
    "@types/istanbul": "0.4.30",
    "@types/karma": "6.3.3",
    "@types/lodash": "4.14.185",
    "@types/minimatch": "5.1.2",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.31",
    "@types/rimraf": "3.0.2",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.8",
    "@types/source-map-support": "0.5.6",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "c8": "7.12.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "concurrently": "7.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "43.0.2",
    "execa": "6.1.0",
    "glob": "8.0.3",
    "install-local": "3.0.1",
    "jasmine": "4.4.0",
    "jasmine-core": "4.4.0",
    "json-schema-to-typescript": "11.0.2",
    "lerna": "5.5.2",
    "minimatch": "5.1.0",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.21",
    "typescript": "4.8.3"
  },
  "scripts": {
    "all": "npm run clean && npm run build && npm run lint && npm run test",
    "postinstall": "lerna bootstrap",
    "lint": "eslint . --resolve-plugins-relative-to . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf \"packages/*/{.nyc_output,reports,coverage,src-generated,*.tsbuildinfo,.stryker-tmp,dist,testResources/tmp}\" \"packages/core/schema/stryker-schema.json\"",
    "generate": "node tasks/generate-json-schema-to-ts.js && node tasks/generate-mono-schema.js",
    "prebuild": "npm run generate",
    "build": "tsc -b && lerna run build",
    "test": "lerna run test --stream --concurrency 2",
    "download-incremental-reports": "./tasks/download-incremental-reports.sh",
    "test:mutation:incremental": "lerna run stryker --stream --concurrency 1 -- -- --incremental",
    "e2e": "cd e2e && npm ci && npm run lint && npm t && cd ..",
    "e2e:update-snapshots": "CHAI_JEST_SNAPSHOT_UPDATE_ALL=true npm run e2e",
    "perf": "cd perf && npm ci && npm run lint && npm t && cd ..",
    "start": "tsc -b -w",
    "release": "lerna version"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
