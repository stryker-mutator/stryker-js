{
  "name": "stryker-parent",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@types/chai": "5.2.3",
    "@types/chai-as-promised": "8.0.2",
    "@types/chai-jest-snapshot": "1.3.8",
    "@types/istanbul": "0.4.34",
    "@types/karma": "6.3.9",
    "@types/mocha": "10.0.10",
    "@types/node": "24.3.0",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "4.0.0",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/utils": "8.46.2",
    "@vitejs/plugin-react": "5.1.0",
    "c8": "10.1.3",
    "chai": "6.2.0",
    "chai-as-promised": "8.0.2",
    "chai-jest-snapshot": "2.0.0",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-chai-friendly": "1.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "estree-walker": "3.0.3",
    "execa": "9.6.0",
    "glob": "11.0.3",
    "install-local": "4.1.0",
    "jasmine": "5.12.0",
    "jasmine-core": "5.12.0",
    "json-schema-to-typescript": "15.0.4",
    "lerna": "9.0.0",
    "minimatch": "10.0.3",
    "mocha": "11.7.4",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.41.0"
  },
  "scripts": {
    "all": "pnpm run clean && pnpm run build && pnpm run lint && pnpm run test",
    "install:chromium": "playwright install chromium",
    "lint": "eslint",
    "sync-dependencies-meta-injected": "sync-dependencies-meta-injected",
    "lint:fix": "pnpm run lint -- --fix",
    "clean": "rimraf --glob \"packages/*/{.nyc_output,reports,coverage,src-generated,*.tsbuildinfo,.stryker-tmp,dist,testResources/tmp}\" \"e2e/test/*/{.stryker-tmp,.stryker-tmp-2}\" \"packages/core/schema/stryker-schema.json\"",
    "generate": "node tasks/generate-json-schema-to-ts.js && node tasks/generate-mono-schema.js",
    "build": "pnpm run generate && tsc -b",
    "build:no-check": "pnpm run build --noCheck",
    "test": "lerna run test --stream --concurrency 2",
    "download-incremental-reports": "./tasks/download-incremental-reports.sh",
    "test:mutation:incremental": "lerna run stryker --stream --concurrency 1 -- --incremental",
    "e2e": "pnpm install && pnpm run e2e:lint && pnpm run e2e:run",
    "e2e:lint": "cd e2e && pnpm run lint",
    "e2e:run": "cd e2e && pnpm test",
    "e2e:update-snapshots": "CHAI_JEST_SNAPSHOT_UPDATE_ALL=true pnpm run e2e",
    "perf": "cd perf && pnpm install && pnpm run setup && pnpm run lint && pnpm test && cd ..",
    "start": "tsc -b -w",
    "release": "pnpm_config_ignore_scripts=false GH_TOKEN=$(gh auth token) lerna version"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
