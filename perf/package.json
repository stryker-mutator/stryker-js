{
  "name": "performance-test",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "ts-node": "~8.10.2"
  },
  "scripts": {
    "merge-config": "ts-node -p tasks/merge-config.ts",
    "postinstall": "npm run merge-config && npm run install",
    "install": "npm run install:deps && npm run install:local",
    "install:deps": "ts-node -p tasks/install.ts",
    "install:local": "ts-node -p tasks/install-local-dependencies.ts",
    "test": "ts-node tasks/run-perf-tests.ts"
  }
}
