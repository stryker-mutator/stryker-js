{
  "name": "performance-test",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "ts-node": "~8.10.2"
  },
  "scripts": {
    "postinstall": "npm run merge-config && npm run bootstrap",
    "merge-config": "ts-node -p tasks/merge-config.ts",
    "bootstrap": "npm run bootstrap:deps && npm run bootstrap:local",
    "bootstrap:deps": "ts-node -p tasks/install.ts",
    "bootstrap:local": "ts-node -p tasks/install-local-dependencies.ts",
    "test": "ts-node tasks/run-perf-tests.ts"
  }
}
