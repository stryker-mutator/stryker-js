{
  "$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "{src,lib}/**/*.+(js|vue)",
    "!{src,lib}/**/*.spec.js"
  ],
  "tempDirName": "stryker-tmp",
  "packageManager": "npm",
  "testRunner": "jest",
  "concurrency": 2,
  "coverageAnalysis": "off",
  "reporters": [
    "event-recorder",
    "progress",
    "clear-text",
    "html"
  ]
}
