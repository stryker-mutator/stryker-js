{
  "mutate": [
    "src/*.js"
  ],
  "testFramework": "mocha",
  "coverageAnalysis": "off",
  "reporters": [
    "clear-text",
    "event-recorder"
  ],
  "mutator": "javascript",
  "maxConcurrentTestRunners": 2,
  "commandRunner": {
    "command": "npm run mocha"
  },
  "symlinkNodeModules": false,
  "fileLogLevel": "info"
}
