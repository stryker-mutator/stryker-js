{
  "name": "jest-react-scripts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.1",
    "jest-environment-jsdom": "27.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.2"
  },
  "scripts": {
    "test:unit": "cross-env SKIP_PREFLIGHT_CHECK=true BABEL_ENV=test react-scripts test --env=$(node -p 'require.resolve(\"jest-environment-jsdom\")')",
    "verify": "mocha --no-config --no-package --timeout 0 verify/verify.js",
    "test": "stryker run && npm run verify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stryker-mutator/core": "workspace:*",
    "@stryker-mutator/jest-runner": "workspace:*",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0"
  },
  "dependenciesMeta": {
    "@stryker-mutator/core": {
      "injected": true
    },
    "@stryker-mutator/jest-runner": {
      "injected": true
    }
  }
}
