{
  "name": "tap-old-version",
  "version": "0.0.0",
  "private": true,
  "description": "A project using the oldest supported version of node-tap",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rimraf \"reports\"",
    "verify": "mocha --no-config --no-package --timeout 0 verify/verify.js",
    "test:unit": "tap test/*.spec.js",
    "test": "npm run clean:reports && stryker run && npm run verify"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tap": "12.7.0",
    "@stryker-mutator/core": "workspace:*",
    "@stryker-mutator/tap-runner": "workspace:*"
  },
  "dependenciesMeta": {
    "@stryker-mutator/core": {
      "injected": true
    },
    "@stryker-mutator/tap-runner": {
      "injected": true
    }
  }
}
