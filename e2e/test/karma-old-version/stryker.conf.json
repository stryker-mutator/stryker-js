{
  "mutate": [
    "src/*.js"
  ],
  "testRunner": "karma",
  "reporters": [
    "json",
    "clear-text",
    "html",
    "event-recorder"
  ],
  "karma": {
    "configFile": "karma.conf.js"
  },
  "timeoutMS": 60000,
  "concurrency": 2,
  "coverageAnalysis": "perTest",
  "plugins": [
    "@stryker-mutator/karma-runner"
  ]
}
