{
  "$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "jest",
  "tempDirName": "stryker-tmp",
  "mutate": [
    "src/App/TodoList/Item/index.tsx",
    "src/NotFound.tsx"
  ],
  "concurrency": 2,
  "coverageAnalysis": "perTest",
  "reporters": [
    "event-recorder",
    "progress",
    "clear-text",
    "html"
  ],
  "jest": {
    "projectType": "create-react-app"
  }
}
