{
  "name": "jest-react-ts",
  "description": "A test for jest with react-scripts and typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unit": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=$(node -p 'require.resolve(\"jest-environment-jsdom\")')",
    "verify": "mocha --no-config --no-package --timeout 0 verify/verify.js",
    "test": "stryker run && npm run verify",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "~11.8.1",
    "@laststance/use-app-state": "~1.2.14",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "~28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "~18.0.12",
    "@types/react-dom": "~18.0.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "typescript": "~4.7.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@stryker-mutator/core": "workspace:*",
    "@stryker-mutator/jest-runner": "workspace:*",
    "@stryker-mutator/typescript-checker": "workspace:*",
    "@types/reach__router": "~1.3.10",
    "jest-environment-jsdom": "27.5.1"
  },
  "dependenciesMeta": {
    "@stryker-mutator/core": {
      "injected": true
    },
    "@stryker-mutator/jest-runner": {
      "injected": true
    },
    "@stryker-mutator/typescript-checker": {
      "injected": true
    }
  }
}
