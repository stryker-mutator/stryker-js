{
  "name": "robobar",
  "version": "1.0.0",
  "type": "module",
  "description": "An example for Mutation Testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/stryker-mutator/robobar-example"
  },
  "main": "index.js",
  "scripts": {
    "all": "npm run lint && npm test && npm run test:mutation",
    "lint": "npm run lint:ts && npm run lint:format",
    "lint:ts": "tsc -b jsconfig.json",
    "lint:format": "prettier --check --ignore-path .gitignore .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "lite-server",
    "test:mutation": "node ../packages/core/bin/stryker.js run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@stryker-mutator/core": "7.3.0",
    "@stryker-mutator/jest-runner": "7.3.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lite-server": "^2.6.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "whatwg-fetch": "^3.6.2"
  }
}
